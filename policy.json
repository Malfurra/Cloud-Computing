{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowVPCEndpointAccess",
      "Effect": "Allow",
      "Principal": "*",
      "Action": [
        "s3:GetObject",
        "s3:PutObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::s3",
        "arn:aws:s3:::s3/*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:SourceVpce": ["<VPCE-S3-ID>"]
        }
      }
    }
  ]
}